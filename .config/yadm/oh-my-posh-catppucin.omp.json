{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "peach": "#FAB387",
    "lavender": "#B7BDF8",
    "flamingo": "#F2CDCD",
    "maroon": "#EBA0AC",
    "sky": "#89DCEB",
    "yellow": "#F9E2AF",
    "blue": "#8AADF4",
    "wight": "#FFFFFF",
    "text": "#494D64",
    "green": "#A6E3A1",
    "red": "#F38BA8"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:wight",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "diamond",
          "template": "{{ .UserName }}@{{ .HostName }}",
          "type": "session"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short",
            "max_depth": 2
          },
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "style": "powerline",
          "properties": {
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "display_worktree_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0b4",
          "foreground": "p:text",
          "background": "p:lavender",
          "background_templates": [
            "{{ if (.Working.Changed) }}p:yellow{{ end }}",
            "{{ if (.Staging.Changed) }}p:green{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:green{{ end }}",
            "{{ if gt .Behind 0 }}p:sky{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "type": "git"
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "p:text",
          "background": "p:green",
          "background_templates": [
            "{{ if .Error }}p:red{{ end }}"
          ],
          "trailing_diamond": "",
          "template": "<p:text></> {{ .Code }} ",
          "properties": {
            "always_enabled": false
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
